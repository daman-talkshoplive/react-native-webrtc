{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "lib": ["es2015", "esnext"],
    "jsx": "react-native",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "types": ["react", "react-native", "debug"]
  },
  "exclude": ["node_modules"]
}
